#include <bits/stdc++.h>
using namespace std;

bool check_palindrom(int num)
{
   int tmp=num;
    int rev=0; 
    while(num!=0)
    {
        int last = num%10;
        rev= rev*10 + last;
        num = num/10;
    }
    if(rev==tmp)
    return true;
    else
    return false;
    
}
int main() {
	// your code goes here
	int n;
	cin>>n; 
	if(check_palindrom(n))
	    cout<<"yes";
	else cout<<"no";
	return 0;
}
